

<ContentView xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="TKGenericSurveyLib.view.components.SurveyTimePicker" xmlns:surveyItems="clr-namespace:TKGenericSurveyLib.model;assembly=TKGenericSurveyLib">






    <ContentView.Content>
        <StackLayout VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand" Orientation="Vertical">
            <Label Text="{Binding Label}" HorizontalOptions="Start" />
            <TimePicker HorizontalOptions="End" Time="{Binding SelectedTime, Mode=TwoWay}" />
        </StackLayout>
    </ContentView.Content>
</ContentView>